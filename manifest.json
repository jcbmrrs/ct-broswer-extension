{
    "name": "UW-IT Contact Tracing Helper",
    "description": "Making it faster & easier to fill out our contact tracing form",
    "version": "1.2",
    "manifest_version": 3,
    "permissions": [
      "storage", 
      "tabs", 
      "scripting",
      "notifications"
    ],
    "host_permissions": [
      "http://*/",
      "https://*/"
    ],

    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "/img/icon16.png",
        "24": "/img/icon24.png",
        "32": "/img/icon32.png",
        "48": "/img/icon48.png",
        "64": "/img/icon64.png",
        "128": "/img/icon128.png",
        "256": "/img/icon256.png",
        "512": "/img/icon512.png"
      }
    },
    "icons": {
      "16": "/img/icon16.png",
      "24": "/img/icon24.png",
      "32": "/img/icon32.png",
      "48": "/img/icon48.png",
      "64": "/img/icon64.png",
      "128": "/img/icon128.png",
      "256": "/img/icon256.png",
      "512": "/img/icon512.png"
    }
  }