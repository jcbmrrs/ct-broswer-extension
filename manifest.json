{
    "name": "UW-IT Contact Tracing Helper",
    "description": "Making it faster & easier to fill out our contact tracing form",
    "version": "1.1",
    "manifest_version": 3,
    "permissions": [
      "storage", 
      "tabs", 
      "scripting",
      "notifications"
    ],
    "host_permissions": [
      "http://*/",
      "https://*/"
    ],

    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
  }